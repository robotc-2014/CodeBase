//Sumo Wrestler
task main()
{
	nSyncedMotors = synchBC;
	
	motor [motorB] = 50;
	motor [motorC] = -50;
	wait1Msec (950);
	
	motor [motorB] = -50;
	motor [motorC] = -50;
	wait1Msec (1000);
	
	motor [motorB] = -50;
	motor [motorC] = 50;
	wait1Msec (950);
	
	motor [motorB] = 100;
	motor [motorC] = 100;
	wait1Msec (35000);


}
